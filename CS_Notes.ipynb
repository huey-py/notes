      "source": [
        "# 2D Dictionary for school supplies with quantity and price\n",
        "\n",
        "supplies = {\n",
        "    \"Pencil\": {\"quantity\": 20, \"price\": 5},\n",
        "    \"Notebook\": {\"quantity\": 15, \"price\": 25},\n",
        "    \"Eraser\": {\"quantity\": 10, \"price\": 3},\n",
        "    \"Crayons\": {\"quantity\": 8, \"price\": 30}\n",
        "}\n",
        "\n",
        "# Function to display the inventory in table format\n",
        "def display_inventory():\n",
        "    print(\"\\nSchool Supplies Inventory\")\n",
        "    print(\"-------------------------------------------------\")\n",
        "    print(f\"{'Item':<12} | {'Qty':<8} | {'Price':<14} | {'Total':<5}\")\n",
        "    print(\"-------------------------------------------------\")\n",
        "    for item, info in supplies.items():\n",
        "        total = info['quantity'] * info['price']\n",
        "        print(f\"{item:<12} | {info['quantity']:<8} | {info['price']:<14} | {total:<5}\")\n",
        "    print(\"-------------------------------------------------\")\n",
        "\n",
        "# Function to add a new item\n",
        "def add_item():\n",
        "    name = input(\"Enter the item name: \")\n",
        "    try:\n",
        "        qty = int(input(\"Enter quantity: \"))\n",
        "        price = float(input(\"Enter price: \"))\n",
        "        supplies[name] = {\"quantity\": qty, \"price\": price}\n",
        "        print(f\"{name} added to inventory.\")\n",
        "    except ValueError:\n",
        "        print(\"Invalid input. Please enter numbers for quantity and price.\")\n",
        "\n",
        "# Function to remove an item\n",
        "def remove_item():\n",
        "    name = input(\"Enter the item name to remove: \")\n",
        "    if name in supplies:\n",
        "        del supplies[name]\n",
        "        print(f\"{name} removed.\")\n",
        "    else:\n",
        "        print(\"Item not found.\")\n",
        "\n",
        "# Function to update the quantity of an item\n",
        "def update_item():\n",
        "    name = input(\"Enter the item name to update: \")\n",
        "    if name in supplies:\n",
        "        try:\n",
        "            qty = int(input(\"Enter new quantity: \"))\n",
        "            supplies[name]['quantity'] = qty\n",
        "            print(f\"{name} quantity updated.\")\n",
        "        except ValueError:\n",
        "            print(\"Invalid input. Please enter a number for quantity.\")\n",
        "    else:\n",
        "        print(\"Item not found.\")\n",
        "\n",
        "# Main program loop\n",
        "def main():\n",
        "    while True:\n",
        "        display_inventory()\n",
        "        print(\"Options: \")\n",
        "        print(\"1 - Add a new item\")\n",
        "        print(\"2 - Remove an item\")\n",
        "        print(\"3 - Update quantity of an item\")\n",
        "        print(\"4 - Exit\")\n",
        "\n",
        "        choice = input(\"Enter your choice (1-4): \")\n",
        "\n",
        "        if choice == \"1\":\n",
        "            add_item()\n",
        "        elif choice == \"2\":\n",
        "            remove_item()\n",
        "        elif choice == \"3\":\n",
        "            update_item()\n",
        "        elif choice == \"4\":\n",
        "            print(\"Exiting program.\")\n",
        "            break\n",
        "        else:\n",
        "            print(\"Invalid choice! Please enter 1, 2, 3, or 4.\\n\")\n",
        "\n",
        "# Run the program\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
      ]
    },
